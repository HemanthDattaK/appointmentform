{
  "version": 2,
  "builds": [
    {
      "src": "appoint/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "productionfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/productionfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "appoint/wsgi.py"
    }
  ]
}
